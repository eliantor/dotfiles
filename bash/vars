#!/bin/sh
export LEIN_HOME=$ETO_LOCAL/etc/lein

export NODE_PATH=$ETO_LOCAL/opt/lib/node:$ETO_LOCAL/opt/lib/node_modules
export N_PREFIX=$ETO_LOCAL/opt
export MANPATH=$MANPATH:$ETO_LOCAL/opt/share/man

export RBENV_ROOT=$ETO_LOCAL/rbenv
export RBX_ROOT=$RBENV_ROOT/versions/rbx-2.0.0-dev

export PYVER="3.3"
export PYTHON_ROOT="/Library/Frameworks/Python.framework/Versions/${PYVER}/"

export CLICOLOR=1
export LSCOLORS=exfxcxdxbxexexabagacad 
